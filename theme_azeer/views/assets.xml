<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <template id="asset_frontend" inherit_id="portal.frontend_layout" name="Theme azzer Frontend">
            <xpath expr="//head/link[last()]" position="after">

                <link href="/theme_azeer/static/src/css/bootstrap.css" rel="stylesheet" type="text/css"/>
                <link href="/theme_azeer/static/src/css/font-awesome.min.css" rel="stylesheet" type="text/css"/>
                <link href="/theme_azeer/static/src/css/style.css" rel="stylesheet" type="text/css"/>
                <link href="/theme_azeer/static/src/css/responsive.css" rel="stylesheet" type="text/css"/>
                <link href="/theme_azeer/static/src/css/custom.css" rel="stylesheet" type="text/css"/>
                <link href="https://fonts.cdnfonts.com/css/nexa-bold" rel="stylesheet"/>

            </xpath>

            <xpath expr="//head/script[last()]" position="after">
                <script src="/theme_azeer/static/src/js/jquery-3.4.1.min.js" type="text/javascript"/>
                <script src="/theme_azeer/static/src/js/myaddon.js" type="text/javascript"/>


            </xpath>
        </template>


    </data>
</odoo>