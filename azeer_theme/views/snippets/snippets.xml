<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <template id="azeer_custom_snippets_web_snippets" inherit_id="website.snippets" name="Azeer Custom Snippets">
            <xpath expr="//div[@id='snippet_structure']"   position="after">
                <div id="azeer_snippet" class="o_panel">
                    <div class="o_panel_header">Azeer Snippets</div>
                    <div class="o_panel_body">
                        <!-- Main carousel-->
                        <!-- <t t-snippet="azeer_theme.azeer_carousel"
                        t-thumbnail="" /> -->
                        <t t-snippet="azeer_theme.azeer_carousel_2"
                        t-thumbnail="/website/static/src/img/snippets_thumbs/s_image_gallery.svg" />  

                        <!-- El Azeer Services -->
                        <t t-snippet="azeer_theme.azeer_working_process"
                        t-thumbnail="/website/static/src/img/snippets_thumbs/s_references.svg" />

                        <!-- OVER +100 OF MEDICAL ANALYTICAL DEVICES READY TO USE ! -->
                        <t t-snippet="azeer_theme.azeer_snippets_3"
                        t-thumbnail="/azeer_theme/static/src/images/snippets/analytic.png" />

                        <!-- El - Azeer Workflow -->
                        <t t-snippet="azeer_theme.azeer_workflow_snippets"
                        t-thumbnail="/azeer_theme/static/src/images/snippets/workflow.png" />

                        <!-- Our Partners -->
                        <t t-snippet="azeer_theme.azeer_our_partner_title_subtitle_snippets"
                        t-thumbnail="/website/static/src/img/snippets_thumbs/s_cover.svg" />
                        <t t-snippet="azeer_theme.azeer_our_partner_block_snippets"
                        t-thumbnail="/website/static/src/img/snippets_thumbs/s_comparisons.svg" />
                        <t t-snippet="azeer_theme.azeer_our_partner_load_more_snippets"
                        t-thumbnail="/azeer_theme/static/src/images/snippets/loadmore.png" />

                        <!-- Keep In Touch -->
                        <t t-snippet="azeer_theme.azeer_s_website_form"
                        t-thumbnail="/website/static/src/img/snippets_thumbs/s_website_form.svg" />

                        <!-- Banner -->
                        <t t-snippet="azeer_theme.azeer_banner_snippets"
                        t-thumbnail="/website/static/src/img/snippets_thumbs/s_cover.svg" />

                        <!-- Portofolio Projects Snippets -->
                        <t t-snippet="azeer_theme.azeer_project_with_shade_snippets"
                        t-thumbnail="/website/static/src/img/snippets_thumbs/s_features.svg" />
                        <t t-snippet="azeer_theme.azeer_project_without_shade_snippets"
                        t-thumbnail="/website/static/src/img/snippets_thumbs/s_features.svg" />

                        <!-- AboutUS Snippets-->
                        <t t-snippet="azeer_theme.azeer_aboutus_story_block" 
                            t-thumbnail="/website/static/src/img/snippets_thumbs/s_text_block.svg"/>
                        <t t-snippet="azeer_theme.azeer_aboutus_values_block"
                            t-thumbnail="/website/static/src/img/snippets_thumbs/s_features.svg"/>
                        <t t-snippet="azeer_theme.azeer_aboutus_mission_block"
                            t-thumbnail="/website/static/src/img/snippets_thumbs/s_cover.svg"/>
                        <t t-snippet="azeer_theme.azeer_aboutus_statistics_block"
                            t-thumbnail="/website/static/src/img/snippets_thumbs/s_features.svg"/>
                        <t t-snippet="azeer_theme.azeer_aboutus_experience_block"
                            t-thumbnail="/website/static/src/img/snippets_thumbs/s_timeline.svg"/>
                            
                        <!-- Contact-Us Snippets -->
                        <t t-snippet="azeer_theme.azeer_contact_us_form"
                        t-thumbnail="/website/static/src/img/snippets_thumbs/s_website_form.svg" />

                        <!-- Trending Now -->
                        <t t-snippet="azeer_theme.trending_now"
                        t-thumbnail=""/>

                        <!--  prodcut List default template  -->
                        <t t-snippet="azeer_theme.s_dynamic_snippet_products_azeer_project" t-thumbnail="/website_sale/static/src/img/snippets_thumbs/s_dynamic_products.svg"/>
                    </div>
                </div>
            </xpath>
        </template>
    </data>
</odoo>